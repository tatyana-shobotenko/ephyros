{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true,
    "node": true
  },
  "rules": {
    "import/no-unresolved": 1,
    "global-require": 0,
    "jsx-no-bind": 0,
    "react/jsx-no-bind": 0,
    "prefer-template": 1,
    "max-len": [
      "error",
      120
    ],
    "new-cap": 0,
    "react/prefer-stateless-function": 0
  }
}
