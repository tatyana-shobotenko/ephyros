    "watch-server": "babel-node -- ./node_modules/webpack/bin/webpack.js --watch --config webpack-watch-server.config --progress --profile --colors",
    "start-dev": "pm2-dev ./build/server/dev.js",
    "start": "pm2-dev ./build/server/prod.js",
    "test": "babel-node -- ./node_modules/karma-cli/bin/karma start"
  },
