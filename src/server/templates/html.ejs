<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title><%- title %></title>
  <meta name="description" content="<%- description %>">
  <!--<meta name="viewport" content="width=device-width">-->
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
  <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
  <meta name="og:image" content="/images/fbshare.png">
  <meta name="og:title" content="<%- title %>">
  <meta name="og:description" content="<%- description %>">
  <% if(stylesUrl){ %>
  <link rel="stylesheet" href="<%- stylesUrl %>">
  <% } %>
  <link
    href='http://fonts.googleapis.com/css?family=Roboto:400,100,300,500,100italic,400italic,500italic,700&subset=latin,cyrillic'
    rel='stylesheet' type='text/css'>
  <!--[if IE]>
  <!--<link rel="stylesheet" type="text/css" href="styles/ie.css"/>
  <![endif]-->
</head>
<body>
<!--[if lt IE 7]>
<p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade
  your browser</a> to improve your experience.</p>
<![endif]-->

<div id="app"><%- appHtml %></div>

<!--[if lt IE 9]>
<script src="<%- ieScriptsUrl %>"></script>
<![endif]-->
<script>
  window.process = <%- JSON.stringify({
    env: {
      GA_CODE: process.env.GA_CODE,
      GMAPS_API_KEY: process.env.GMAPS_API_KEY,
    }
  }) %>;
</script>
<script defer src="<%- scriptsUrl %>"></script>
<!-- Hello here ;) -->
</body>
</html>
