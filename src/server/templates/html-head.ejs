<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title><%- title %></title>
  <meta name="description" content="<%- description %>">
  <!--<meta name="viewport" content="width=device-width">-->
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
  <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
  <meta name="og:image" content="/images/fbshare.png">
  <meta name="og:title" content="<%- title %>">
  <meta name="og:description" content="<%- description %>">
  <% if(stylesUrl){ %>
  <link rel="stylesheet" href="<%- stylesUrl %>">
  <% } %>

  <link
    href='https://fonts.googleapis.com/css?family=Roboto:400,100,300,500,100italic,400italic,500italic,700&subset=latin,cyrillic'
    rel='stylesheet' type='text/css'>
  <% if(process.env.OPTIMIZELY_PROJECT_ID) { %>
  <link rel="preload" as="script"
        href="<%- 'https://cdn.optimizely.com/js/' + process.env.OPTIMIZELY_PROJECT_ID + '.js' %>">
  <% } %>
</head>
<body>
